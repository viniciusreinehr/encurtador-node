  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.0",
    "@types/mongoose": "^5.10.3",
    "@types/shortid": "0.0.29",
