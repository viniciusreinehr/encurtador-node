{
  "name": "encurtador-node",
  "version": "1.0.0",
  "description": "Encurtador de URL",
  "main": "app.js",
  "scripts": {
    "test": "jest",
    "prebuild": "rm -rf dist/*",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
